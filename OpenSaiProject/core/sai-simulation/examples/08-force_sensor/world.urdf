<?xml version="1.0" ?>

<world name="demo_world" gravity="0.0 0.0 -9.81">

	<robot name="PPPBot">
		<model dir="${EXAMPLE_08_FOLDER}" path="pppbot.urdf" name="PPPBot" />
		<origin xyz="0.0 1.0 0.0" rpy="0 0 0" />
	</robot>

	<static_object name="Box">
		<origin xyz="0.0 0.0 0.5" rpy="0 0 0" />
		<collision>
			<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
			<geometry>
				<box size="0.5 3.0 0.1" />
			</geometry>
		</collision>
		<visual>
			<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
			<geometry>
				<box size="0.5 3.0 0.1" />
			</geometry>
			<material name="material_red">
				<color rgba="0.5 0.0 0.0 1.0" />
			</material>
		</visual>
	</static_object>

	<static_object name="Wall">
		<origin xyz="0.0 -0.5 0.5" rpy="0 0 0" />
		<collision>
			<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
			<geometry>
				<box size="0.5 0.1 3.0" />
			</geometry>
		</collision>
		<visual>
			<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
			<geometry>
				<box size="0.5 0.1 3.0" />
			</geometry>
			<material name="material_red">
				<color rgba="0.3 0.3 0.0 1.0" />
			</material>
		</visual>
	</static_object>

	<light name="light1" type="directional">
		<position xyz="2.0 -2.0 2.0" />
		<lookat xyz="0.0 0.0 0.5" />
	</light>

	<light name="light2" type="directional">
		<position xyz="2.0 2.0 2.0" />
		<lookat xyz="0.0 0.0 0.5" />
	</light>

	<camera name="camera_fixed">
		<position xyz="4.0 0.0 4.0" />
		<vertical xyz="0.0 0.0 1.0" />
		<lookat xyz="0.0 0.0 0.0" />
	</camera>

</world>
