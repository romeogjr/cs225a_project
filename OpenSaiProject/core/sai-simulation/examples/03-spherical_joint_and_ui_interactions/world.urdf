<?xml version="1.0" ?>

<world name="demo_world" gravity="0.0 0.0 -9.81">

	<robot name="SphericalBot">
		<model dir="${EXAMPLE_03_FOLDER}" path="spherical_bot.urdf" name="SphericalBot_description" />
		<origin xyz="0.0 1.0 1.0" rpy="0 0 0" />
		<visual showwireframe="off" showframes="off" />
	</robot>

	<robot name="RRRBot">
		<model dir="${EXAMPLE_03_FOLDER}" path="rrrbot.urdf" name="RRRBot_description" />
		<origin xyz="0.0 -1.0 1.0" rpy="0 0 0" />
		<visual showwireframe="off" showframes="off" />
	</robot>

	<light name="light1" type="directional">
		<position xyz="2.0 -2.0 2.0" />
		<lookat xyz="0.0 0.0 0.5" />
	</light>

	<light name="light2" type="directional">
		<position xyz="2.0 2.0 2.0" />
		<lookat xyz="0.0 0.0 0.5" />
	</light>

	<camera name="camera_fixed">
		<position xyz="4.0 0.0 4.0" />
		<vertical xyz="0.0 0.0 1.0" />
		<lookat xyz="0.0 0.0 0.0" />
	</camera>

</world>
